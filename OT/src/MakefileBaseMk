	cp -r $(JHSWING) $(WIN_RELEASE_ROOT)/support
	cp -r $(ODB_CP) $(WIN_RELEASE_ROOT)/ODBServer
	-mkdir -p $(WIN_RELEASE_ROOT)/bin
	cp -r $(INSTALL_BASE_DIR)/bin/ot $(WIN_RELEASE_ROOT)/bin
	cp -r $(WIN_JRE_ROOT) $(WIN_RELEASE_ROOT)

