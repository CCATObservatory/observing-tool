<spDocument>
   <lb name=new type=lb subtype=none>
      <av name=title descr="No Description">
            <val value="UKIRT LIBRARY">
      </av>
      <av name=.gui.collapsed descr="No Description">
            <val value="false">
      </av>
      <av name=.gui.filename. descr="No Description">
            <val value="UKIRT-Template.lib">
      </av>
      <av name=.gui.filename descr="No Description">
            <val value="UKIRT-Template.lib">
      </av>
      <lf name=new type=lf subtype=none>
         <av name=.gui.collapsed descr="No Description">
                  <val value="true">
         </av>
         <av name=title descr="No Description">
                  <val value="UFTI library">
         </av>
         <no name=new type=no subtype=none>
            <av name=.gui.collapsed descr="No Description">
                        <val value="false">
            </av>
            <av name=note descr="No Description">
                        <val value="Choose the type of UFTI observation you would like to make, copy it to your Science Program and then edit the values you need to change.\n\nRemember to include an array_tests observation so that you can check the array performance at the start of each night.\n\n9 point or 5-point jitter patterns are available with standard offsets of 10 or 20 arcsecs.  (Edit for non-standard offsets.)\n\nQuadrant-jitter patterns for deep imaging are available.\n\nSky-and-jitter sequences are available for\naccurate photometry of faint sources.\n\nA 5x5 mosaic for an extended source is available.\n\n\n">
            </av>
            <av name=title descr="No Description">
                        <val value="selections in this folder">
            </av>
         </no>
         <ob name=new type=ob subtype=none>
            <av name=title descr="No Description">
                        <val value="array_tests">
            </av>
            <av name=chainedToNext descr="No Description">
                        <val value="false">
            </av>
            <av name=.gui.collapsed descr="No Description">
                        <val value="true">
            </av>
            <av name=standard descr="No Description">
                        <val value="false">
            </av>
            <av name=chainedToPrev descr="No Description">
                        <val value="false">
            </av>
            <no name=new type=no subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=note descr="No Description">
                              <val value="The UFTI configuration to use the blank filter in the component below">
               </av>
            </no>
            <oc name=new type=oc subtype=inst.UFTI>
               <av name=posAngle descr="No Description">
                              <val value="0.0">
               </av>
               <av name=instPort descr="No Description">
                              <val value="East">
               </av>
               <av name=.version descr="No Description">
                              <val value="1.0">
               </av>
               <av name=readoutArea descr="No Description">
                              <val value="1024x1024">
               </av>
               <av name=coadds descr="No Description">
                              <val value="2">
               </av>
               <av name=sourceMag descr="No Description">
                              <val value="none">
               </av>
               <av name=instAper descr="No Description">
                              <val value="8.13">
                              <val value="-0.23">
                              <val value="0.0">
                              <val value="2.222">
               </av>
               <av name=polariser descr="No Description">
                              <val value="none">
               </av>
               <av name=filter descr="No Description">
                              <val value="Blank">
               </av>
               <av name=exposureTime descr="No Description">
                              <val value="4.0">
               </av>
               <av name=.unique descr="No Description">
                              <val value="true">
               </av>
               <av name=acqMode descr="No Description">
                              <val value="Normal+NDSTARE">
               </av>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
            </oc>
            <no name=new type=no subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=note descr="No Description">
                              <val value="Use the ARRAY_TESTS data-reduction recipe.">
               </av>
            </no>
            <oc name=new type=oc subtype=DRRecipe>
               <av name=SkyRecipe descr="No Description">
                              <val value="ARRAY_TESTS">
               </av>
               <av name=FlatRecipe descr="No Description">
                              <val value="REDUCE_FLAT">
               </av>
               <av name=ArcRecipe descr="No Description">
                              <val value="REDUCE_ARC">
               </av>
               <av name=DarkRecipe descr="No Description">
                              <val value="ARRAY_TESTS">
               </av>
               <av name=title descr="No Description">
                              <val value="ARRAY_TESTS">
               </av>
               <av name=.unique descr="No Description">
                              <val value="true">
               </av>
               <av name=BiasInGroup descr="No Description">
                              <val value="false">
               </av>
               <av name=DarkInGroup descr="No Description">
                              <val value="true">
               </av>
               <av name=BiasRecipe descr="No Description">
                              <val value="REDUCE_BIAS">
               </av>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=ObjectRecipe descr="No Description">
                              <val value="ARRAY_TESTS">
               </av>
               <av name=DRRecipe descr="No Description">
                              <val value="ARRAY_TESTS">
               </av>
               <av name=ArcInGroup descr="No Description">
                              <val value="false">
               </av>
               <av name=FlatInGroup descr="No Description">
                              <val value="false">
               </av>
               <av name=SkyInGroup descr="No Description">
                              <val value="false">
               </av>
               <av name=ObjectInGroup descr="No Description">
                              <val value="false">
               </av>
            </oc>
            <if name=new type=if subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <no name=new type=no subtype=none>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=note descr="No Description">
                                    <val value="Take three 5-second darks, so that the first one can be thrown away">
                  </av>
               </no>
               <ic name=new type=ic subtype=darkObs>
                  <av name=coadds descr="No Description">
                                    <val value="1">
                  </av>
                  <av name=exposureTime descr="No Description">
                                    <val value="5.0">
                  </av>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=repeatCount descr="No Description">
                                    <val value="3">
                  </av>
               </ic>
               <no name=new type=no subtype=none>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=note descr="No Description">
                                    <val value="Now do a 60-second dark - using the dark observe iterator below to change the exposure time. ">
                  </av>
                  <av name=title descr="No Description">
                                    <val value="now do a long dark">
                  </av>
               </no>
               <ic name=new type=ic subtype=darkObs>
                  <av name=coadds descr="No Description">
                                    <val value="1">
                  </av>
                  <av name=exposureTime descr="No Description">
                                    <val value="60.0">
                  </av>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=repeatCount descr="No Description">
                                    <val value="1">
                  </av>
               </ic>
            </if>
         </ob>
         <ob name=new type=ob subtype=none>
            <av name=title descr="No Description">
                        <val value="JHK photometry, 5pt jitters">
            </av>
            <av name=chainedToNext descr="No Description">
                        <val value="false">
            </av>
            <av name=.gui.collapsed descr="No Description">
                        <val value="true">
            </av>
            <av name=standard descr="No Description">
                        <val value="false">
            </av>
            <av name=chainedToPrev descr="No Description">
                        <val value="false">
            </av>
            <no name=new type=no subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=note descr="No Description">
                              <val value="This sequence is an example of how to do generic n-colour photometry efficiently in a single observation.\n\nAlthough this is for JHK, you can change the filters to do any set.   Note that at the moment only the jitter_self_flat_ncolour data reduction recipe is available, for historical reasons.  Others will be made colour generic soon.">
               </av>
               <av name=title descr="No Description">
                              <val value="Usage">
               </av>
            </no>
            <no name=new type=no subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=note descr="No Description">
                              <val value="Enter your target co-ordinates into the target list below.  Note that although this is called a \"target list\" it really only allows you to specify ONE astronomical target.\n\nWhen you have done so you can use the \"position editor\" to  select a guide star, to fine tune the co-ordinate based on a sky survey picture, examine offset locations on the field etc.\n\nFor a step-by-step guide to how to use the position editor use \"help\".">
               </av>
               <av name=title descr="No Description">
                              <val value="target information">
               </av>
            </no>
            <oc name=new type=oc subtype=targetList>
               <av name=.unique descr="No Description">
                              <val value="true">
               </av>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=Base descr="No Description">
                              <val value="Base">
                              <val value="Object Name">
                              <val value="0:00:00">
                              <val value="0:00:00">
                              <val value="FK5 (J2000)">
               </av>
               <av name=.gui.selectedTelescopePos descr="No Description">
                              <val value="Base">
               </av>
            </oc>
            <no name=new type=no subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=note descr="No Description">
                              <val value="The UFTI component below sets to the J filter as a base configuration.\n\nThe sequence then takes darks with exposure times appropriate to each filter that will be used.\n\nThe UFTI filters and exposure times for each filter are then \"iterated\" (cycled through) in the sequence below.\n\nThe UFTI component below has exposures set for a 11-12 mag star.   Change these as appropriate for your source.\n\n\n\n\n">
               </av>
               <av name=title descr="No Description">
                              <val value="UFTI setup information">
               </av>
            </no>
            <oc name=new type=oc subtype=inst.UFTI>
               <av name=posAngle descr="No Description">
                              <val value="0.0">
               </av>
               <av name=instPort descr="No Description">
                              <val value="East">
               </av>
               <av name=.version descr="No Description">
                              <val value="1.0">
               </av>
               <av name=readoutArea descr="No Description">
                              <val value="512x512">
               </av>
               <av name=coadds descr="No Description">
                              <val value="2">
               </av>
               <av name=sourceMag descr="No Description">
                              <val value="9-10">
               </av>
               <av name=instAper descr="No Description">
                              <val value="-12.86">
                              <val value="17.51">
                              <val value="0.0">
                              <val value="1.250">
               </av>
               <av name=polariser descr="No Description">
                              <val value="none">
               </av>
               <av name=filter descr="No Description">
                              <val value="J98">
               </av>
               <av name=exposureTime descr="No Description">
                              <val value="5.0">
               </av>
               <av name=title descr="No Description">
                              <val value="- set configuration">
               </av>
               <av name=.unique descr="No Description">
                              <val value="true">
               </av>
               <av name=acqMode descr="No Description">
                              <val value="Normal+NDSTARE">
               </av>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
            </oc>
            <no name=new type=no subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=note descr="No Description">
                              <val value="The data-reduction recipe specified below uses the most complete data-reduction recipe.\n\nThis recipe does self flat reduction for each colour.  If you do each colour more than once then the data will be co-added for each.\n\nSeparate reduced files are generated for each colour. \n\nThe algorithm is identical to JITTER_SELF_FLAT.">
               </av>
               <av name=title descr="No Description">
                              <val value="DR Recipe Selection">
               </av>
            </no>
            <oc name=new type=oc subtype=DRRecipe>
               <av name=SkyRecipe descr="No Description">
                              <val value="REDUCE_SKY">
               </av>
               <av name=FlatRecipe descr="No Description">
                              <val value="REDUCE_FLAT">
               </av>
               <av name=ArcRecipe descr="No Description">
                              <val value="REDUCE_ARC">
               </av>
               <av name=DarkRecipe descr="No Description">
                              <val value="REDUCE_DARK">
               </av>
               <av name=title descr="No Description">
                              <val value="JITTER_SELF_FLAT_NCOLOUR">
               </av>
               <av name=.unique descr="No Description">
                              <val value="true">
               </av>
               <av name=BiasInGroup descr="No Description">
                              <val value="false">
               </av>
               <av name=DarkInGroup descr="No Description">
                              <val value="false">
               </av>
               <av name=BiasRecipe descr="No Description">
                              <val value="REDUCE_BIAS">
               </av>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=ObjectRecipe descr="No Description">
                              <val value="JITTER_SELF_FLAT_NCOLOUR">
               </av>
               <av name=DRRecipe descr="No Description">
                              <val value="JITTER_SELF_FLAT">
               </av>
               <av name=ArcInGroup descr="No Description">
                              <val value="false">
               </av>
               <av name=FlatInGroup descr="No Description">
                              <val value="false">
               </av>
               <av name=SkyInGroup descr="No Description">
                              <val value="true">
               </av>
               <av name=ObjectInGroup descr="No Description">
                              <val value="true">
               </av>
            </oc>
            <if name=new type=if subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <no name=new type=no subtype=none>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=note descr="No Description">
                                    <val value="If you changed the dark exposure time in the UFTI configuration above remember to change it here too.\n\nN.B.: The data-reduction recipe will not work unless you have a dark with the right exposure time.\n\nThree darks are done here - one for each filter that will follow.  Set the exposure times and co-adds to match those which you will do on the source in each band.\n">
                  </av>
                  <av name=title descr="No Description">
                                    <val value="set dark exposure time here">
                  </av>
               </no>
               <ic name=new type=ic subtype=darkObs>
                  <av name=coadds descr="No Description">
                                    <val value="1">
                  </av>
                  <av name=exposureTime descr="No Description">
                                    <val value="15.0">
                  </av>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=repeatCount descr="No Description">
                                    <val value="1">
                  </av>
               </ic>
               <ic name=new type=ic subtype=darkObs>
                  <av name=coadds descr="No Description">
                                    <val value="1">
                  </av>
                  <av name=exposureTime descr="No Description">
                                    <val value="10.0">
                  </av>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=repeatCount descr="No Description">
                                    <val value="1">
                  </av>
               </ic>
               <no name=new type=no subtype=none>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=note descr="No Description">
                                    <val value="To repeat the 5-point jitter pattern more than once for each filter on your target, use the repeat item below to specify how many times you want to repeat it.\n\n">
                  </av>
                  <av name=title descr="No Description">
                                    <val value="how to do more than one 5-point jitter">
                  </av>
               </no>
               <ic name=new type=ic subtype=repeat>
                  <av name=repeatCount descr="No Description">
                                    <val value="1">
                  </av>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=title descr="No Description">
                                    <val value="Repeat 1X">
                  </av>
                  <no name=new type=no subtype=none>
                     <av name=.gui.collapsed descr="No Description">
                                          <val value="false">
                     </av>
                     <av name=note descr="No Description">
                                          <val value="The UFTI iteration component below changes the filter, starting with J.  No exposure time is entered for J because it is already set in the general UFTI setup above.   The filter and exposure time are then changed for H and then again for K.">
                     </av>
                     <av name=title descr="No Description">
                                          <val value="UFTI filter iteration">
                     </av>
                  </no>
                  <ic name=new type=ic subtype=instUFTI>
                     <av name=instAperLIter descr="No Description">
                                          <val value="1.250">
                                          <val value="1.635">
                                          <val value="2.150">
                     </av>
                     <av name=iterConfigList descr="No Description">
                                          <val value="filterIter">
                                          <val value="instAperLIter">
                                          <val value="exposureTimeIter">
                     </av>
                     <av name=.gui.collapsed descr="No Description">
                                          <val value="false">
                     </av>
                     <av name=filterIter descr="No Description">
                                          <val value="J98">
                                          <val value="H98">
                                          <val value="K98">
                     </av>
                     <av name=exposureTimeIter descr="No Description">
                                          <val value="15">
                                          <val value="10">
                                          <val value="10">
                     </av>
                     <no name=new type=no subtype=none>
                        <av name=.gui.collapsed descr="No Description">
                                                <val value="false">
                        </av>
                        <av name=note descr="No Description">
                                                <val value="The non-square pattern of offsets in the jitter below avoids re-inforcement of poor row/column.\n\nIf you want to use a different size of offset, move the highlight to the offset item below and edit them.">
                        </av>
                        <av name=title descr="No Description">
                                                <val value="offset info">
                        </av>
                     </no>
                     <ic name=new type=ic subtype=offset>
                        <av name=Offset1 descr="No Description">
                                                <val value="11.0">
                                                <val value="10.0">
                        </av>
                        <av name=.gui.selectedOffsetPos descr="No Description">
                                                <val value="Offset8">
                        </av>
                        <av name=offsetPositions descr="No Description">
                                                <val value="Offset8">
                                                <val value="Offset3">
                                                <val value="Offset5">
                                                <val value="Offset7">
                                                <val value="Offset1">
                        </av>
                        <av name=title descr="No Description">
                                                <val value="jitter_5_10as">
                        </av>
                        <av name=.gui.collapsed descr="No Description">
                                                <val value="false">
                        </av>
                        <av name=Offset8 descr="No Description">
                                                <val value="0.0">
                                                <val value="0.0">
                        </av>
                        <av name=Offset7 descr="No Description">
                                                <val value="11.0">
                                                <val value="-11.0">
                        </av>
                        <av name=Offset5 descr="No Description">
                                                <val value="-12.0">
                                                <val value="-10.0">
                        </av>
                        <av name=Offset3 descr="No Description">
                                                <val value="-10.0">
                                                <val value="11.0">
                        </av>
                        <ic name=new type=ic subtype=observe>
                           <av name=repeatCount descr="No Description">
                                                      <val value="1">
                           </av>
                           <av name=.gui.collapsed descr="No Description">
                                                      <val value="false">
                           </av>
                        </ic>
                     </ic>
                  </ic>
               </ic>
               <no name=new type=no subtype=none>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=note descr="No Description">
                                    <val value="This last offset simply takes the telescope back to 0,0 after all the repeats of the jitter pattern are finished.">
                  </av>
                  <av name=title descr="No Description">
                                    <val value="last offset">
                  </av>
               </no>
               <ic name=new type=ic subtype=offset>
                  <av name=.gui.selectedOffsetPos descr="No Description">
                                    <val value="Offset0">
                  </av>
                  <av name=.gui.collapsed descr="No Description">
                                    <val value="false">
                  </av>
                  <av name=Offset0 descr="No Description">
                                    <val value="0.0">
                                    <val value="0.0">
                  </av>
                  <av name=offsetPositions descr="No Description">
                                    <val value="Offset0">
                  </av>
               </ic>
            </if>
         </ob>
         <ob name=new type=ob subtype=none>
            <av name=title descr="No Description">
                        <val value="5pt jitter/offsets: Bright Point Source">
            </av>
            <av name=chainedToNext descr="No Description">
                        <val value="false">
            </av>
            <av name=.gui.collapsed descr="No Description">
                        <val value="true">
            </av>
            <av name=standard descr="No Description">
                        <val value="false">
            </av>
            <av name=chainedToPrev descr="No Description">
                        <val value="false">
            </av>
            <no name=new type=no subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=note descr="No Description">
                              <val value="5-point jitter offsets (repeated) on a bright point source.  It is assumed that the background signal in the short-exposure images is insufficient to construct a good flat field.  This sequence therefore requires a pre-observed flat field (use \"make_skyflats\").\n\nFor point sources, to cut down on overheads, use the UFTI\nsub-array and smaller jitter-steps.">
               </av>
               <av name=title descr="No Description">
                              <val value="Usage">
               </av>
            </no>
            <no name=new type=no subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=note descr="No Description">
                              <val value="Enter your target co-ordinates into the target list below.  Note that although this is called a \"target list\" it really only allows you to specify ONE astronomical target.\n\nWhen you have done so you can use the \"position editor\" to  select a guide star, to fine tune the co-ordinate based on a sky survey picture, examine offset locations on the field etc.\n\nFor a step-by-step guide to how to use the position editor use \"help\".">
               </av>
               <av name=title descr="No Description">
                              <val value="target information">
               </av>
            </no>
            <oc name=new type=oc subtype=targetList>
               <av name=.unique descr="No Description">
                              <val value="true">
               </av>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=Base descr="No Description">
                              <val value="Base">
                              <val value="Object Name">
                              <val value="0:00:00">
                              <val value="0:00:00">
                              <val value="FK5 (J2000)">
               </av>
               <av name=.gui.selectedTelescopePos descr="No Description">
                              <val value="Base">
               </av>
            </oc>
            <no name=new type=no subtype=none>
               <av name=.gui.collapsed descr="No Description">
                              <val value="false">
               </av>
               <av name=note descr="No Description">
                              <val value="The UFTI component below uses the K filter.  To change it move the highlight to the \"UFTI component\" below and select the filter you want to use.\n\nThe UFTI component below uses a 5-second exposure time.  To change it move the highlight to \"UFTI\" below and change the exposure time - either enter the value you want to use, or enter the magnitude range of your target and the recommended exposure time will be automatically entered.\n\n">
               </av>
               <av name=title descr="No Description">
                              <val value="how to set the exposure time and filter">
               </av>
            </no>
            <oc name=new type=oc subtype=inst.UFTI>
