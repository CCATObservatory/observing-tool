
echo Starting registry...
${JAVA_BIN}/rmiregistry 4201 &
echo Registry started
sleep 1
echo Loading server...
${JAVA_BIN}/java -ms5m -mx50m -ss5m -Dot.cfgdir=$otcfgdir -DCFGDIR=$cfgdir ODBServer &
set clobber
jobs -l > /tmp/odbjobs
echo "#\!/bin/csh -f" > $PRG_HOME/killODBSvr
awk '{print "kill ",$3}' /tmp/odbjobs >> $PRG_HOME/killODBSvr
chmod +x $PRG_HOME/killODBSvr
rm -f /tmp/odbjobs

